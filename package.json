{
  "name": "@fioc/strict",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.cjs"
    }
  },
  "keywords": [
    "dependency injection",
    "functional dependency injection",
    "di container",
    "typescript",
    "inversion of control",
    "type safety",
    "strict typing",
    "container management",
    "modular dependency injection"
  ],
  "description": "A lightweight, type-safe extension for @fioc/core, enforcing strict compile-time validation for dependency injection in JS/TS applications",
  "author": "kolostring <yoerodriguezjunior@gmail.com>",
  "contributors": [
    {
      "name": "leonardof02",
      "email": "leonardoarencibia2002@gmail.com"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kolostring/fioc-strict.git"
  },
  "scripts": {
    "build": "tsup",
    "lint": "eslint .",
    "release": "tsup && npm publish --access public"
  },
  "dependencies": {
    "@fioc/core": "^1.6.0",
    "immer": "^10.1.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@types/node": "^24.3.1",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "tsup": "^8.5.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.1",
    "vitest": "^3.2.4"
  }
}
